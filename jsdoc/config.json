{
  "tags": {
    "allowUnknownTags": true
  },
  "opts": {
    "destination": "./dist/docs",
    "readme": "./README.md",
    "watch": true
  },
  "plugins": [
    "plugins/markdown"
  ],
  "templates": {
    "cleverLinks": false,
    "monospaceLinks": false,
    "theme": "cerulean",
    "navType": "vertical",
    "linenums": true,
    "dateFormat": "MMMM Do YYYY, h:mm:ss a"
  }
}
